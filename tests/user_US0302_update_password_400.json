                                    "          let value = JSONPath({",
                                    "            json: jsonData,",
                                    "            path: expression,",
                                    "            wrap: false",
                                    "          });",
                                    "          ",
                                    "        ",
                                    "        switch (typeof value) {",
                                    "          case 'object':",
                                    "            value = JSON.stringify(value);",
                                    "            break;",
                                    "          default:",
                                    "            value = String(value);",
                                    "            break;",
                                    "        }",
                                    "      ",
                                    "        pm.environment.set(`token`, value);console.log('已设置环境变量【'+ `token` + '】，值为【' + value + '】')",
