                                    "type": "string",
                                    "mock": {
                                        "mock": "@string"
                                    },
                                    "title": "权"
                                },
                                "id": {
                                    "type": "integer",
                                    "title": "用户id"
                                }
                            },
                            "required": [
                                "token",
                                "id"
                            ]
                        },
